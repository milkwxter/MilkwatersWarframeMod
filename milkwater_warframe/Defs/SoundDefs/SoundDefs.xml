        <eventNames/>
        <maxSimultaneous>1</maxSimultaneous>
        <subSounds>
            <li>
                <grains>
                    <li Class="AudioGrain_Clip">
                        <clipPath>Weapons/Milkwater_Shot_Strun</clipPath>
