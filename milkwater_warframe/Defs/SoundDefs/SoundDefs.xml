	<!--=============== Nukor ====================-->
    <SoundDef>
    <defName>Milkwater_Shot_Nukor</defName>
    <sustain>true</sustain>
    <context>MapOnly</context>
    <maxSimultaneous>2</maxSimultaneous>
    <priorityMode>PrioritizeNearest</priorityMode>
    <sustainStopSound>Milkwater_Shot_Nukor_Resolve</sustainStopSound>
    <subSounds>
      <li>
        <tempoAffectedByGameSpeed>true</tempoAffectedByGameSpeed>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Weapons/Milkwater_Loop_Nukor</clipPath>
          </li>
        </grains>
        <pitchRange>0.99~1.01</pitchRange>
        <volumeRange>30</volumeRange>
        <sustainRelease>0.02</sustainRelease>
      </li>
    </subSounds>
  </SoundDef>

  <SoundDef>
    <defName>Milkwater_Shot_Nukor_Resolve</defName>
